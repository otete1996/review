<div class="col-md-12">
  <h1>レビュー投稿</h1>
  <%= form_with model: @review,local: true do |form| %>
  <div class="form-group">
    <%= form.label("本文") %>
    <%= form.text_area :context,class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label("sore") %>
    <%= form.number_field :sore,class: "form-control" %>

  </div>
  <%= form.submit("登録する",class: "btn btn-primary") %>
  <% end %>



</div>
